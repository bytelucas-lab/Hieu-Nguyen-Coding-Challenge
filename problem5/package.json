{
  "name": "curd-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "pm2:start": "NODE_ENV=production pm2 start ./build/src/app.js -i 1 --attach | ./node_modules/.bin/bunyan",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "devWindows": "nodemon -r tsconfig-paths\\register src\\app.ts | .\\node_modules\\.bin\\bunyan",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan",
    "aiSubmitterWorker:dev": "nodemon -r tsconfig-paths/register src/shared/workers/external/aiSubmitter.worker.ts | ./node_modules/.bin/bunyan",
    "pm2:aiSubmitterWorker:start": "NODE_ENV=production pm2 start ./build/src/shared/workers/external/aiSubmitter.worker.js -i 1 --attach | ./node_modules/.bin/bunyan",
    "aiFetcherWorker:dev": "nodemon -r tsconfig-paths/register src/shared/workers/external/aiFetcher.worker.ts | ./node_modules/.bin/bunyan",
    "pm2:aiFetcherWorker:start": "NODE_ENV=production pm2 start ./build/src/shared/workers/external/aiFetcher.worker.js -i 1 --attach | ./node_modules/.bin/bunyan",
    "lint": "biome check src/ --write",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "prettier:check": "prettier --check src/**/*.{ts,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,json}",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "seeds:dev": "npx ts-node src/seeds.ts",
    "seeds:prod": "npx ts-node ./build/src/seeds.js"
  },
  "keywords": [],
  "author": "Lucas Nguyen",
  "license": "MIT",
